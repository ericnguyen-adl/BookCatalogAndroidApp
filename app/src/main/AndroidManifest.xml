<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="au.edu.tafesa.itstudies.bookcatalog">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.EditBookActivity"></activity>
        <activity
            android:name=".activities.BookCatalogActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.BookShoppingCartActivity"
            android:label="@string/title_activity_book_shopping_cart" />
        <activity android:name=".activities.NewBookActivity" />
    </application>

</manifest>